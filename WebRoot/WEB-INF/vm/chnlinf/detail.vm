<html>
<head> 
<link rel="stylesheet" type="text/css" href="${request.contextPath}/profile/default/css/common.css">
<script src="$request.contextPath/js/ui/jquery.min.js"></script>
<script src="$request.contextPath/js/ui/js/om-core.js"></script>
<script src="$request.contextPath/js/ui/js/om-validate.js"></script>
<script type="text/javascript" src="$request.contextPath/js/ui/js/om-ajaxsubmit.js"></script>
<script src="$request.contextPath/js/ui/js/om-button.js"></script>
<script src="$request.contextPath/js/ui/js/om-dialog.js"></script>
  <script>
   //关闭窗口方法
   function  closeDialog(){
          window.parent.fillBackAndCloseDialog("#dialog"); 
     } 
  </script>  
  </head>
  <body>
	    <table width="100%" cellpadding="0" cellspacing="0" class="infowrite">
	      <tr>
	            <th width="40%">渠道编号：</th>
	            <td class="td" >$!{chnl.channelId}</td>
	      </tr>
	      <tr>
	            <th>渠道名称：</th>
	            <td class="td1" >$!{chnl.channelName}</td>
	      </tr>
	      <tr>
		          <th >运营负责人：</th>
		          <td class="td" >$!{chnl.service_user}
		          </td>
		    </tr>
	       <tr>
	            <th >联系人：</th>
	            <td class="td1" >$!{chnl.contact}</td>
	      </tr>
	     <tr>
	            <th >联系电话：</th>
	            <td class="td" >$!{chnl.linkTel}</td>
	      </tr>
	      <tr>
	            <th >电子邮件：</th>
	            <td class="td1" >$!{chnl.email}</td>
	      </tr>
	      </table>
	  <div class="pop_btn">
	      <input name="close" type="button" class="popbtn" onmouseover="this.className='popbtnhover'" onmouseout="this.className='popbtn'" value="关闭" onclick="closeDialog();"/>
	  </div>
  </body>
  </html>
