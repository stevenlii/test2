<html>
    <head>
        <link rel="stylesheet" href="${request.contextPath}/profile/default/css/common.css"
        type="text/css">
        <link rel="stylesheet" href="${request.contextPath}/js/ui/themes/default/om-all.css"
        type="text/css">
        <script src="$request.contextPath/js/ui/jquery.min.js">
        </script>
        <script src="$request.contextPath/js/ui/js/om-core.js">
        </script>
        <script type="text/javascript" src="$request.contextPath/js/ui/js/om-grid.js">
        </script>
        <script src="$request.contextPath/js/ui/js/om-mouse.js">
        </script>
        <script src="$request.contextPath/js/ui/js/om-draggable.js">
        </script>
        <script src="$request.contextPath/js/ui/js/om-position.js">
        </script>
        <script src="$request.contextPath/js/ui/js/om-resizable.js">
        </script>
        <script src="$request.contextPath/js/ui/js/om-dialog.js">
        </script>
        <script src="$request.contextPath/js/ui/js/om-validate.js">
        </script>
        <script src="$request.contextPath/js/ui/js/om-messagebox.js">
        </script>
        <script src="$request.contextPath/js/ui/js/om-button.js">
        </script>
        <script src="$request.contextPath/js/ui/js/om-combo.js">
        </script>
        <script src="$request.contextPath/js/busi/meraudit.js">
        </script>
        <script>
            jQuery(document).ready(function() {
                var optStr = "${opts}";
                var opts = optStr.split(',');
                jQuery('[code]').each(function() {
                    var code = $(this).attr("code");
                    for (var i = 0; i < opts.length; i++) {
                        if (code == opts[i]) {
                            $(this).css('display', 'inline');
                        }
                    };
                });
            });
            //商户审核通过方法
            function goPass(id) {
                var resultDesc = document.getElementById("resultDesc").value;
                if (byteCount(resultDesc) > 64) {
                    alert("审核意见最大长度为64个字符！") return false;
                }
                var flag = confirm("确认审核通过此信息？");
                if (flag) {
                    $.ajax({
                        url: '$request.contextPath/meraudit/merAuditPass.do',
                        type: 'POST',
                        dataType: 'text',
                        data: {
                            'id': id,
                            'resultDesc': resultDesc
                        },
                        success: function(data) {
                            closeDialog();
                            window.parent.shuaxin();
                            window.parent.showSimpleTip(data);
                        }
                    });
                }
            }
        </script>
    </head>
    
    <body>
        <div>
            <!--新增或启用/禁用商户审核详情页面-->
            <form id="dataform-add" method="post" name="dataform-add">
                <table align="left" class="infowrite" cellspacing="0" cellpadding="0"
                width="100%">
                    <tr>
                        <th width="30%">
                            	商户号：
                        </th>
                        <td class="td" colspan="2">
                            ${audit.merId}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	商户名称：
                        </th>
                        <td class="td1" colspan="2">
                            $!{audit.merName}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	业务属性：
                        </th>
                        <td class="td" colspan="2">
                            $!{audit.busiType}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	商户分类：
                        </th>
                        <td class="td1" colspan="2">
                            $!{audit.category}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	接口类型：
                        </th>
                        <td class="td">
                            $!{audit.interType}
                        </td>
                        #if(${auditType}==3)
                        <td width="40%" class="td" rowspan="2">
                            <h2>
                                	禁用->
                                <span style="color:red">
                                    	启用
                                </span>
                                <h2>
                        </td>
                        #elseif(${auditType}==4)
                        <td width="40%" class="td" rowspan="2">
                            <h2>
                                	启用->
                                <span style="color:red">
                                    	禁用
                                </span>
                                <h2>
                        </td>
                        #else
                        <td class="td">
                        </td>
                        #end
                    </tr>
                    <tr>
                        <th>
                            	是否渠道报备：
                        </th>
                        <td class="td1">
                            $!{audit.chnlCheck}
                        </td>
                        #if(${auditType}==1)
                        <td class="td1">
                        </td>
                        #else #end
                    </tr>
                    <tr>
                        <th>
                            	开通对账文件下载：
                        </th>
                        <td class="td" colspan="2">
                            #if ($!{audit.fileDL} == 0) 否 #else 是 #end
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	运营负责人：
                        </th>
                        <td class="td1" colspan="2">
                            $!{audit.operator}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	公司名称：
                        </th>
                        <td class="td" colspan="2">
                            $!{audit.companyName}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	公司介绍：
                        </th>
                        <td class="td1" colspan="2">
                            $!{audit.companyDesc}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	业务介绍：
                        </th>
                        <td class="td" colspan="2">
                            $!{audit.busiDesc}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	商户网址：
                        </th>
                        <td class="td1" colspan="2">
                            $!{audit.merWeb}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	自有营销渠道：
                        </th>
                        <td class="td" colspan="2">
                            $!{audit.saleChannel}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	注册资本：
                        </th>
                        <td class="td1" colspan="2">
                            $!{audit.regCapital}&nbsp;万元
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	注册时间：
                        </th>
                        <td class="td" colspan="2">
                            $!{audit.regTime}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	商户来源：
                        </th>
                        <td class="td1" colspan="2">
                            $!{audit.srcMer}
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	年收益：
                        </th>
                        <td class="td" colspan="2">
                            $!{audit.yearProfit}&nbsp;万元
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	用户规模：
                        </th>
                        <td class="td1" colspan="2">
                            $!{audit.userScale}&nbsp;人
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	运营支撑：
                        </th>
                        <td class="td" colspan="2">
                            $!{audit.support}&nbsp;人
                        </td>
                    </tr>
                    <tr>
                        <th>
                            	分成比例：
                        </th>
                        <td class="td1" colspan="2">
                            $!{audit.sharedRate}
                        </td>
                    </tr>
                    <tr>
                        <td height="63" colspan="3" align="left" valign="bottom">
                            <textarea id="resultDesc" name="resultDesc" rows="3" style="width:100%"></textarea>
                        </td>
                    </tr>
                </table>
                <div class="pop_btn_sh">
                    <input type="button" value="通过" code="011" style="display:none" class="popbtn"
                    onmouseover="this.className='popbtnhover'" onmouseout="this.className='popbtn'"
                    onclick="goPass($audit.id)" />
                    <input type="button" value="不通过" code="012" style="display:none" class="popbtn"
                    onmouseover="this.className='popbtnhover'" onmouseout="this.className='popbtn'"
                    onclick="goNotPass($audit.id)" />
                    <input type="button" value="关闭" class="popbtn" onmouseover="this.className='popbtnhover'"
                    onmouseout="this.className='popbtn'" onclick="closeDialog(0)" />
                </div>
            </form>
        </div>
    </body>

</html>